<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col-md-12">
    <h1 class="text-align-left" >Documents</h1>
    <%= link_to new_project_document_path("projects", @project)  do %>
    <%= fa_icon "plus", class: "btn btn-info" %>
    <% end %>
  </div>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Created By</th>
      <th>Create At</th>
      <th>Status</th>
      <th>Details</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>

  <tbody>
    <% if @project.document != nil %>
      <%= render "shared/display_models", object: document %>
    <% end %>
  </tbody>
</table>

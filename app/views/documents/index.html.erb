<div class="row">
  <div class="col-md-10">
    <p class="text-align-left font-size-200 text-bold" >Documents</p>
  </div>

    <%= link_to new_document_path("projects", @project)  do %>
    <%= fa_icon "plus", class: "btn btn-info buffer-margin-10" %>
    <% end %>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>Title</th>
      <th>Created By</th>
      <th>Create At</th>
      <th>Status</th>
      <th>Details</th>
      <th>Edit</th>
      <th>Delete</th>
    </tr>
  </thead>

  <tbody>
    <% @project.documents.each do |document| %>
      <%= render "shared/display_models", object: document %>
    <% end %>
  </tbody>
</table>

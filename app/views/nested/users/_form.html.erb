<div class="well background-lighter-grey">
    <%= form_for @user, url: route do |f| %>
        <%= render 'shared/error_messages', object: @user %>

        <%= render 'user_fields', f: f, users: @users || [@user], target_resource: @resource %>

        <div class="actions">
          <%= f.submit %>
        </div>
    <% end %>
</div>

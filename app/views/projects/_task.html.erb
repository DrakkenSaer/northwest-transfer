<div class="row task-list">
  <h3 class="col-xs-2 task-number">Task <%= task_counter + 1 %></h3>
  <h3 class="col-xs-1 bar">|</h3>
  <div class="col-xs-6">
    <h3><%= task.name %></h3>
    <p class="subtext">assigned to Darryl Philbin</p>
  </div>
  <img class="icons" src="images/button-placeholder.png" alt="done">
  <img class="icons" src="images/button-placeholder.png" alt="edit">
  <img class="icons" src="images/button-placeholder.png" alt="right arrow">
</div>
<div class="task-description">
  <h3 class="subheader">Description</h3>
  <h4><%= task.description %></h4>
</div>
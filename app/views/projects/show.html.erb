  <header>
    <!-- ///////////////////////HEADER///////////////////////////// -->
    <div class="row">
      <div class="col-xs-1">
        <img id="user-image" src="images/User_Silhouette_Placeholder.png" alt="User Silhouette Placeholder">
      </div>
      <div class="col-xs-3">
        <h1 id="user-name"><%= @project.customer.first_name + " " + @project.customer.last_name %></h1>
        <h6 id="hours">
          <script>
            var div = document.getElementById('hours');
            var datetime = document.createTextNode( 
                moment( new Date(<%= project.start_date.to_f * 1000 %>) ).format("MMM D, h:mm A") +
                " - " +
                moment( new Date(<%= project.estimated_completion_date.to_f * 1000 %>) ).format("MMM D, h:mm A")
            );
            div.appendChild(datetime);
          </script>
        </h6>
      </div>
      <div class="col-xs-pull-0" id="header-right">
        <!--
        already a clock in the application header so...¯\_(ツ)_/¯
        <h1 id="local-time"><#%= render "shared/clock" %></h1>
        -->
        <h3 id="current-date"><%= Date.today %></h3>
      </div>
    </div>
    <!-- //////////////////TOGGLE JOBS BAR/////////////////////////// -->
    <div class="row" id="toggle-screen-bar">
      <div class="col-xs-6" id="last-job-text">
        <img class="arrow" src="images/left-arrow.png" alt="left arrow"><a href="#">Previous Job Page</a>
      </div>
      <div class="col-xs-6" id="next-job-text">
        <a href="#">Next Job Page</a>
        <img class="arrow" src="images/right-arrow.png" alt="right arrow">
      </div> 
    </div>
  </header>
    <div class="row">
    <!-- //////////////////JOB DETAILS SECTION/////////////////////// -->
      <div class="row col-md-7" id="job-details">
        <div class="section-title">
          <p>Job Details</p>
        </div>
        <div id="job-description">
          <h4 class="subheader">Job Description:</h4>
          <p><%= @project.description %></p>
        </div>

        <!-- /////////////////////TIMELINE/////////////////////////// -->
        <div class="row timeline-section">
          <div class="col-xs-4 timeline">
            <p><span class="subsection-title">Address: </span> <%= @project.address %> </p>
            <p> <%= @project.city %>,  <%= @project.state %>  <%= @project.postal %></p>
          </div>
          <div class="col-xs-4 timeline" id="start-end-date">
            <p><span class="subsection-title">Start: </span> <%= @project.start_date %></p>
            <p><span class="subsection-title">Due: </span> <%= @project.completion_date %></p>
          </div>
          <div class="col-xs-4 timeline">
            <%= render partial: "leader", collection: @project.leaders %>
          </div>
        </div>
        <!-- ///////////////////////TASK LIST/////////////////////// -->
            <!-- ///////TASK ONE PLACEHOLDER//////// -->
        <%= render partial: "tasks", collection: @project.tasks %>
      </div>
      <!-- //////////////////NOTIFICATIONS/////////////////////// -->
      <div class="col-md-pull-0 col-md-4 right-sidebar-section">
        <table class="table table-striped">
          <thead>
            <tr>
              <div class="section-title">
                <p>Notifications</p>
              </div>
            </tr>
          </thead>
          <tbody>
            <tr class="notification-item">
              <td>
                <h4>There is a screw missing for the television mount.</h4>
                <p>posted by Jerry DiCanio | March 12, 2017 | 1:56pm</p>

      <!-- //////////////////DOCUMENTS/////////////////////// -->
      <div class="col-md-pull-0 col-md-4 right-sidebar-section">
        <table class="table table-striped">
          <thead>
            <tr>
              <div class="section-title">
                <p>Document</p>
              </div>
            </tr>
          </thead>
          <tbody>
            <tr>
              <% if @project.document.present? %>
                <td><h4><%= @project.document.title %></h4></td>
                <td>last updated by</td>
                <td><%= @project.document.updated_at.strftime("%B %e, %Y") %></td>
                <td><%= @project.document.updated_at.strftime("%H:%M") %></td>
              <% else %>
                <td>No Document Yet</td>
              <% end %>
            </tr>
          </tbody>
        </table>
      </div>
    </div>
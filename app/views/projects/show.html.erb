      <!-- //////////////////CONTACTS/////////////////////// -->
      <div class="col-md-pull-0 col-md-4 right-sidebar-section">
        <table class="table table-striped">
          <thead>
            <tr>
              <div class="section-title">
                <p>Contacts</p>
              </div>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td> <%= image_tag("User_Silhouette_Placeholder.PNG", alt: 'user placeholder', id: 'contact-image') %></td>

<%if object.class == Task %>

  <%= render "shared/task-display", object: object %>

<% elsif object.class == Note %>

  <%= render "shared/note-display", object: object %>

<% elsif object.class == Document %>

  <%= render "shared/document-display", object: object %>

  <% elsif object.class == User %>

  <%= render "shared/user-display", object: object %>
<% end %>

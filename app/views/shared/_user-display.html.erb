<tr>
  <td><%= object.first_name %></td>
  <td><%= object.last_name %></td>
  <td><%= object.phone %></td>
  <td><%= object.email  %></td>

  <td>
    <%= link_to edit_account_path(object.id) do %>
      <%= fa_icon "pencil", class:"btn btn-success" %>
    <% end %>
  </td>

  <td>
    <%= link_to cancel_user_registration_path(object.id), method: :delete, data: { confirm: 'Are you sure?' } do %>
      <%= fa_icon "trash", class:"btn btn-danger" %>
    <% end %>
  </td>
</tr>
